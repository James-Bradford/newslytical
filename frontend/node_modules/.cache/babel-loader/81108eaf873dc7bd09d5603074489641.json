;;;;;;;;;;AAgCA;;;AAGA,eAAA;AACA,EAAA,IAAA,EAAA,WADA;;AAEA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,SAAA,EAAA,MADA;AAEA,MAAA,WAAA,EAAA,MAFA;AAGA,MAAA,cAAA,EAAA,SAHA;AAIA,MAAA,KAAA,EAAA;AAJA,KAAA;AAMA,GATA;;AAUA,EAAA,KAAA,EAAA;AACA,IAAA,aAAA,EAAA;AADA,GAVA;AAaA,EAAA,OAAA,EAAA;AACA;;;;;AAKA,IAAA,YAAA,CAAA,GAAA,EAAA;AACA,aAAA,GAAA,CAAA,QAAA,GAAA,OAAA,CAAA,yBAAA,EAAA,KAAA,CAAA;AACA,KARA;;AASA;;;;;AAKA,IAAA,SAAA,CAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,GAAA;AACA,KAhBA;;AAiBA;;;;;AAKA,IAAA,cAAA,CAAA,IAAA,EAAA;AACA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA;AACA,UAAA,IAAA,GAAA,KAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,YAAA,QAAA,GAAA,KAAA,KAAA,CAAA,QAAA,CAAA,QAAA;;AAEA,cAAA,IAAA;AACA,aAAA,SAAA;AACA;AACA,eAAA,IAAA,CAAA,IAAA,QAAA,EAAA;AACA,YAAA,KAAA,CAAA,SAAA,GAAA,KAAA,CAAA,SAAA,CAAA,OAAA,CACA,MAAA,QAAA,CAAA,CAAA,CAAA,CAAA,IADA,EAEA,mCAAA,QAAA,CAAA,CAAA,CAAA,CAAA,IAAA,SAFA,CAAA;AAIA;;AACA;;AAEA,aAAA,KAAA;AACA;AACA,eAAA,IAAA,CAAA,IAAA,IAAA,EAAA;AACA,YAAA,KAAA,CAAA,SAAA,GAAA,KAAA,CAAA,SAAA,CAAA,OAAA,CACA,IAAA,CAAA,CAAA,CAAA,CAAA,GADA,EAEA,8BAAA,IAAA,CAAA,CAAA,CAAA,CAAA,YAAA,SAFA,CAAA;AAIA;;AACA;AAnBA;;AAsBA,aAAA,KAAA,CAAA,SAAA;AACA;;AAnDA,GAbA;;AAkEA,EAAA,OAAA,GAAA;AACA,QAAA,KAAA,KAAA,IAAA,CAAA,SAAA,EAAA;AAAA,WAAA,cAAA,CAAA,KAAA,aAAA;AAAA;AAEA,GArEA;;AAsEA,EAAA,QAAA,EAAA;AACA,IAAA,KAAA,GAAA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA;AACA,KAHA;;AAIA,IAAA,GAAA,EAAA;AACA,MAAA,GAAA,EAAA,YAAA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAAA;AACA,OAHA;AAIA,MAAA,GAAA,EAAA,UAAA,GAAA,EAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,QAAA,EAAA,GAAA;AACA;AANA;AAJA;AAtEA,CAAA","sourcesContent":["<template lang=\"pug\">\n      //Card\n      v-card#tweet-card.mx-auto.pa-6.elevated-4.rounded-0\n        v-card-title\n          v-icon(large='' left='' color='white') mdi-twitter\n          span.title.font-weight-light.white--text Original Tweet\n\n          //Tweet Text\n          div#tweet-text.text-h6.text-md-h4.white--text(v-if=\"tweet.entities\" ref='highlight' v-html='highlightTweet()')\n\n        // Tweet Profile\n        v-card-actions\n          v-list-item.grow\n\n            //Profile Image\n            v-list-item-avatar(color='grey darken-3')\n              img.elevation-6(v-if=\"tweet.user\" :src=\"String(tweet.user.profile_image_url_https)\")\n\n            //Profile Name\n            v-list-item-content\n              span.pa-1(v-if=\"this.highlightType=='profile'\" style=\"background-color: green\")\n                v-list-item-title.white--text(v-if=\"tweet.user\") {{ tweet.user.name }}\n            v-row(align='center' justify='end')\n              v-icon.mr-1(color='white') mdi-heart\n              span.subheading.mr-2.white--text(v-if=\"tweet.favorite_count\") {{ formatNumber(tweet.favorite_count) }}\n              span.mr-1.white--text &middot;\n              v-icon.mr-1(color='white') mdi-twitter-retweet\n              span.subheading.white--text(v-if=\"tweet.retweet_count\") {{ formatNumber(tweet.retweet_count) }}\n\n</template>\n\n<script>\n/**\n * Card containing tweet information\n */\nexport default {\n  name: \"TweetCard\",\n  data() {\n    return {\n      full_text: String,\n      profile_url: String,\n      hightlightType: \"profile\",\n      sheet: false,\n    };\n  },\n  props: {\n    highlightType: String,\n  },\n  methods: {\n    /**\n     * Formats a number to include commas\n     *\n     * @param num Number\n     */\n    formatNumber(num) {\n      return num.toString().replace(/(\\d)(?=(\\d{3})+(?!\\d))/g, \"$1,\");\n    },\n    /**\n     * Sets the current tab to a given number\n     *\n     * @param tab Tab Number\n     */\n    tabChange(tab) {\n      this.tab = tab;\n    },\n    /**\n     * Highlights text on the tweet\n     * \n     * @param type Type of Highlight\n     */\n    highlightTweet(type) {\n      //Set variables\n      var tweet = this.tweet;\n      var urls = this.tweet.entities.urls;\n      const hashtags = this.tweet.entities.hashtags;\n\n      switch (type) {\n        case \"hashtag\":\n          //For each hashtag, replace it with highlighted version\n          for (let i in hashtags) {\n            tweet.full_text = tweet.full_text.replace(\n              \"#\" + hashtags[i].text,\n              `<span class=\"highlightHashtag\">#${hashtags[i].text}</span>`\n            );\n          }\n          break;\n\n        case \"url\":\n          //For each URL, replace it with highlighted version\n          for (let i in urls) {\n            tweet.full_text = tweet.full_text.replace(\n              urls[i].url,\n              `<span class=\"highlightURL\">${urls[i].expanded_url}</span>`\n            );\n          }\n          break;\n      }\n\n      return tweet.full_text;\n    }\n  },\n  mounted() {\n    if(this.tweet ==! undefined) {this.highlightTweet(this.highlightType);}\n    \n  },\n  computed: {\n    tweet() {\n      return this.$store.state.tweet;\n    },\n    tab: {\n      get: function() {\n        return this.$store.state.tab;\n      },\n      set: function(tab) {\n        this.$store.dispatch(\"setTab\", tab);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n</style>"],"sourceRoot":"src/components"}]}