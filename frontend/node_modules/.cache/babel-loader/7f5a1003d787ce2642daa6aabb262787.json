,CAAgBC,OAAhB,CAAwBL,KAAxB,EAA+B,gCAAgCH,IAAI,CAACE,CAAD,CAAJ,CAAQO,YAAxC,GAAuD,SAAtF,CAAf;AACH;;AAED,WAAOH,QAAP;AACH;;AAnBwB","sourcesContent":["import axios from 'axios';\n\nvar api = 'http://localhost:4000/api';\n\nexport default class Twitter {\n\n\n    tweet(id) {\n        var response;\n        axios.get(api + '/twitter/' + id)\n            .then( function(response) { return response.data; } )\n            .catch(err => console.log(err));\n    }\n\n    tweetUrls(tweet) {\n        const urls = tweet.entities.urls;\n\n        for (let i in urls) {\n            var regex = new RegExp(urls[i].url, \"g\");\n            var newTweet = tweet.full_text.replace(regex, \"<span class='urlHighlight'>\" + urls[i].expanded_url + \"</span>\");\n        }\n\n        return newTweet;\n    }\n\n}\n"]}]}