;AACA,IAAA,KADA;AAEA,IAAA,OAFA;AAGA,IAAA;AAHA,GAFA;;AAOA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,GAAA,EAAA,IADA;AAEA,MAAA,KAAA,EAAA,IAFA;AAGA,MAAA,SAAA,EAAA,IAHA;AAIA,MAAA,SAAA,EAAA,KAJA;AAKA,MAAA,OAAA,EAAA;AACA,QAAA,GAAA,EAAA,IADA;AAEA,QAAA,OAAA,EAAA;AAFA;AALA,KAAA;AAUA,GAlBA;;AAmBA,EAAA,OAAA,EAAA;AACA,UAAA,KAAA,GAAA;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA,CADA,CAGA;;AACA,WAAA,SAAA,GAAA,IAAA;AACA,YAAA;AAAA,QAAA;AAAA,UAAA,MAAA,OAAA,CAAA,QAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,CAAA;AACA,WAAA,SAAA,GAAA,KAAA,CANA,CAQA;;AACA,WAAA,KAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,OAAA,CAAA,SAAA,CAAA,KAAA,KAAA,EAAA,KAAA,OAAA,CAAA;AACA,KAZA;;AAaA;AACA,IAAA,SAAA,CAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,GAAA;AACA,KAhBA;;AAiBA;AACA,IAAA,YAAA,CAAA,CAAA,EAAA;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;;AACA,cAAA,CAAA;AACA,aAAA,KAAA;AACA,eAAA,OAAA,CAAA,GAAA,GAAA,CAAA,KAAA,OAAA,CAAA,GAAA;AACA,eAAA,SAAA,GAAA,OAAA,CAAA,SAAA,CAAA,KAAA,KAAA,EAAA,KAAA,OAAA,CAAA;AACA;;AACA,aAAA,SAAA;AACA,eAAA,OAAA,CAAA,OAAA,GAAA,CAAA,KAAA,OAAA,CAAA,OAAA;AACA,eAAA,SAAA,GAAA,OAAA,CAAA,SAAA,CAAA,KAAA,KAAA,EAAA,KAAA,OAAA,CAAA;AACA;;AACA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,2BAAA;AAVA;AAYA;;AAjCA,GAnBA;;AAsDA,EAAA,OAAA,GAAA;AACA,SAAA,KAAA;AACA;;AAxDA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- Tab Content Start -->\n    <v-tabs-items v-model=\"tab\" class=\"tab-background fill-height\">\n      <!-- Tweet Tab -->\n      <v-tab-item :key=\"1\" class=\"fill-height\">\n        <v-overlay v-if=\"isLoading\">\n          <v-progress-circular indeterminate color=\"primary\" size=\"200\" v-if=\"isLoading\"></v-progress-circular>\n        </v-overlay>\n        <Tweet\n          v-if=\"!isLoading\"\n          v-bind:tweetText=\"tweetText\"\n          v-bind:tweet=\"tweet\"\n          v-on:tabChange=\"this.changeTab($event)\"\n          @toggle-filter=\"this.toggleFilter\"\n          v-bind:filters=\"filters\"\n        />\n      </v-tab-item>\n      <!-- Tweet Tab -->\n\n      <!-- Profile Tab -->\n      <v-tab-item :key=\"2\" class=\"fill-height\">\n        <Profile v-if=\"!isLoading\" v-bind:tweet=\"tweet\" />\n      </v-tab-item>\n      <!-- Profile Tab -->\n\n      <!-- Link Tab -->\n      <v-tab-item :key=\"3\" class=\"fill-height\">\n          <Link v-if=\"!isLoading\" />\n      </v-tab-item>\n      <!-- Link Tab -->\n\n      <!-- Words Tab -->\n      <v-tab-item :key=\"4\">\n        <v-container>\n          <h1>Words</h1>\n        </v-container>\n      </v-tab-item>\n      <!-- Words Tab -->\n\n      <v-tab-item :key=\"5\">\n        <v-container>\n          <h1>Media</h1>\n        </v-container>\n      </v-tab-item>\n      <v-tab-item :key=\"6\">\n        <v-container>\n          <h1>Summary</h1>\n        </v-container>\n      </v-tab-item>\n    </v-tabs-items>\n    <!-- Tab Content End -->\n\n    <!-- Tab Button Start -->\n    <v-tabs\n      v-model=\"tab\"\n      icons-and-text\n      style=\"position: fixed; bottom: 0; left: 0; width: 100%\"\n      center-active\n      centered\n      grow\n    >\n      <v-tabs-slider></v-tabs-slider>\n\n      <v-tab :key=\"1\">\n        Tweet\n        <v-icon>mdi-twitter</v-icon>\n      </v-tab>\n\n      <v-tab :key=\"2\">\n        Profile\n        <v-icon>mdi-account</v-icon>\n      </v-tab>\n\n      <v-tab :key=\"3\">\n        Links\n        <v-icon>mdi-link</v-icon>\n      </v-tab>\n\n      <v-tab :key=\"4\">\n        Words\n        <v-icon>mdi-cursor-text</v-icon>\n      </v-tab>\n\n      <v-tab :key=\"5\">\n        Media\n        <v-icon>mdi-image</v-icon>\n      </v-tab>\n\n      <v-tab :key=\"6\">\n        Summary\n        <v-icon>mdi-card-text-outline</v-icon>\n      </v-tab>\n    </v-tabs>\n    <!-- Tab Button End -->\n  </div>\n</template>\n\n<script>\n//Import components\nimport Tweet from \"../components/analysis/tweet/Tweet\";\nimport Profile from \"../components/analysis/profile/Profile.vue\";\nimport Link from \"../components/analysis/link/Link\";\n\n//Import twitter API\nimport { Twitter } from \"../services/Twitter\";\n\nexport default {\n  name: \"Analysis\",\n  components: {\n    Tweet,\n    Profile,\n    Link\n  },\n  data() {\n    return {\n      tab: null,\n      tweet: null,\n      tweetText: null,\n      isLoading: false,\n      filters: {\n        url: true,\n        hashtag: true\n      },\n    };\n  },\n  methods: {\n    async fetch() {\n      var twitter = new Twitter();\n\n      //Load required data from backend\n      this.isLoading = true;\n      const { data } = await twitter.getTweet(this.$route.params.id);\n      this.isLoading = false;\n\n      //Set the data equal to the local variables\n      this.tweet = data;\n      this.tweetText = twitter.highlight(this.tweet, this.filters);\n    },\n    //Changes the active tab to a defined tab\n    tabChange(tab) {\n      this.tab = tab;\n    },\n    //Changes the active highlight filters\n    toggleFilter(e) {\n      var twitter = new Twitter();\n      console.log(e);\n      switch (e) {\n        case \"url\":\n          this.filters.url = !this.filters.url;\n          this.tweetText = twitter.highlight(this.tweet, this.filters);\n          break;\n        case \"hashtag\":\n          this.filters.hashtag = !this.filters.hashtag;\n          this.tweetText = twitter.highlight(this.tweet, this.filters);\n          break;\n        default:\n          console.error(\"Filter error has occurred\");\n      }\n    },\n  },\n  created() {\n    this.fetch();\n  }\n};\n</script>\n\n<style scoped>\n.tab-item-wrapper {\n  /* vuetify sets the v-tabs__container height to 48px */\n\n  margin-top: 100px;\n  margin-bottom: 100px;\n}\n</style>"],"sourceRoot":"src/views"}]}